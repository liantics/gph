<div class="row">
  <% @category_types.each do |type| %>
    <div class="col-sm-<%= @column_width %> show_block_no_shadow">
      <h2><%= type %></h2>
      <%= render partial: "category_type", locals: {project: @projects, category_type: type} %>
    </div>
  <% end %>

  <div class="col-sm-<%= @column_width %> show_block_no_shadow">
    <%= render "finish_line_projects" %>
  </div>
</div>
